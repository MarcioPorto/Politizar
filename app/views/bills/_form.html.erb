
<%= simple_form_for(@bill) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :identifier %>
    <%= f.input :description %>
    <%= f.input :vote_date %>
    <%= f.input :result %>
    <%= f.input :result_description %>
    <%= f.association :institution %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
